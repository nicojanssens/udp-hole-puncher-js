<?xml version='1.0' encoding='utf-8'?>
<widget id="io.microminion.udp-hole-puncher-test" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>UdpHolePuncher CordovaTest</name>
    <description>
        Very basic test to verify whether or not UdpHolePuncher loads properly in Cordova app
    </description>
    <author email="nico.b.janssens@gmail.com">
        Nico Janssens
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-chrome-apps-sockets-udp" spec="https://github.com/MobileChromeApps/cordova-plugin-chrome-apps-sockets-udp" />
</widget>
